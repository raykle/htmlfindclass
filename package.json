{
	"name": "htmlfindclass",
	"displayName": "HtmlFindClass",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.htmlfindclass"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.htmlfindclass",
				"title": "HtmlFindClass"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "resourceLangId == html",
					"command": "extension.htmlfindclass",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"html-dom-parser": "^0.1.3",
		"htmlparser2": "^3.10.0"
	}
}
